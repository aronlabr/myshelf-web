<h2>Buscar Libros</h2>
<form action="/authors" method="get">
    <label>Nombre</label>
    <input type="text" name="name" value="<% searchOptions.name %>">
    <button type="submit">Buscar</button>
</form>
<br>
<% authors.forEach(author => { %>
    <div><%= author.name %> </div>
    <a href="/authors/<%= author.id %>">Ver</a>
    <a href="/authors/<%= author.id %>/edit">Editar</a>
    <%- include('../partials/deleteForm.ejs', {url: `/authors/${author.id}`}) %> 
<% }) %> 